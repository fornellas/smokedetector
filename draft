query
  from|to DATE
  field FIELD
    matches REGEXP
    min|max VALUE
  first COUNT
  skip COUNT
report
    average|avg|mean FIELD
    count
    distinct_count|dc FIELD
    maximum|max FIELD
    minimum|min FIELD
    median FIELD
    mode FIELD
    sum FIELD
  by
    field_name NAME
    field_range NAME VALUE
    second|minute|hour|day|week
sort as
  numbers
  strings
    insensitive
  reverse
